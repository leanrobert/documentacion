{
  "name": "documentacion",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint src/content.config.ts astro.config.mjs",
    "lint:fix": "eslint src/content.config.ts astro.config.mjs --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "astro check",
    "type-check": "astro check --watch",
    "test": "echo \"No tests specified\" && exit 0",
    "validate": "npm run lint && npm run format:check",
    "prepare": "husky"
  },
  "dependencies": {
    "@astrojs/node": "^9.5.0",
    "@astrojs/starlight": "^0.36.1",
    "astro": "^5.6.1",
    "better-auth": "^1.3.34",
    "better-sqlite3": "^11.10.0",
    "crypto-js": "^4.2.0",
    "sharp": "^0.34.2"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.0",
    "@commitlint/cli": "^18.0.0",
    "@commitlint/config-conventional": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-astro": "^0.31.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "husky": "^9.0.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0",
    "typescript": "^5.6.0"
  },
  "lint-staged": {
    "*.{js,mjs,ts,astro}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,mdx}": [
      "prettier --write"
    ]
  }
}
